### Symfony ###
# Cache and logs (Symfony2)
/app/cache/*
/app/logs/*
!app/cache/.gitkeep
!app/logs/.gitkeep

# Email spool folder
/app/spool/*
/mysql/*
/elastic/*
/elastic--a/*
# Cache, session files and logs (Symfony3)
/app/var/cache/*
/app/var/logs/*
/app/var/sessions/*
!/app/var/cache/.gitkeep
!/app/var/logs/.gitkeep
!app/var/sessions/.gitkeep

# Logs (Symfony4)
/app/var/log/*
!app/var/log/.gitkeep

# Parameters
/app/config/parameters.yml
/app/config/parameters.ini

# Managed by Composer
/app/bootstrap.php.cache
/app/var/bootstrap.php.cache
/app/bin/*
!app/bin/console
!app/bin/symfony_requirements
/app/vendor/*

# Assets and user uploads
/app/web/bundles/
/app/web/uploads/

# PHPUnit
/app/phpunit.xml
/app/phpunit.xml

# Build data
/app/build/

# Composer PHAR
/composer.phar

# Backup entities generated with doctrine:generate:entities command
**/Entity/*~

# Embedded web-server pid file
/.web-server-pid

### Symfony Patch ###
/web/css/
/web/js/

### Vuejs ###
# Recommended template: Node.gitignore

/app/node_modules/
dist/
npm-debug.log
yarn-error.log